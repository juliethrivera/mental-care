<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.EjerciciosRespiracionFragment">


    <TextView
        android:id="@+id/textView"
        android:layout_width="274dp"
        android:layout_height="51dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="60dp"
        android:text="    Ejercicios de respiracion profunda "
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button1"
        android:layout_width="192dp"
        android:layout_height="60dp"
        android:layout_marginBottom="48dp"
        android:text="Empezar Ejercicio"
        app:layout_constraintBottom_toBottomOf="parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="109dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="68dp"
        android:layout_height="52dp"
        android:src="@drawable/arrow_back_24dp_fill0_wght400_grad0_opsz24"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="172dp"
        android:layout_height="137dp"
        android:layout_marginStart="16dp"
        android:src="@drawable/Respiracion_Profunda_Sentado"
        app:layout_constraintStart_toStartOf="parent"
        android:ap
        tools:ignore="MissingConstraints,MissingPrefix"
        tools:layout_editor_absoluteY="95dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="176dp"
        android:layout_height="132dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/Respiracion_Profunda_Hasta_Sonrreir"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="99dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="171dp"
        android:layout_height="125dp"
        android:src="@drawable/Respiracion_Profunda_Voca_Bajo"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="248dp"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="176dp"
        android:layout_height="118dp"
        android:src="@drawable/Respiracion_Profunda_Voca_Riba"
        tools:layout_editor_absoluteX="215dp"
        tools:layout_editor_absoluteY="254dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>